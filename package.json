{
  "name": "eco-image-picker",
  "version": "1.0.12",
  "description": "基于react的移动端图片上传组件",
  "scripts": {
    "start": "dumi dev",
    "docs:build": "dumi build",
    "docs:deploy": "gh-pages -d docs-dist",
    "build": "father-build",
    "deploy": "npm run docs:build && npm run docs:deploy",
    "release": "npm run build && npm publish",
    "prettier": "prettier --write \"**/*.{js,jsx,tsx,ts,less,md,json}\"",
    "test": "umi-test",
    "test:coverage": "umi-test --coverage",
    "complete": "npm run docs:build && npm run release"
  },
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "typings": "dist/index.d.ts",
  "author": "Ecode",
  "license": "MIT",
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx,less,md,json}": [
      "prettier --write"
    ],
    "*.ts?(x)": [
      "prettier --parser=typescript --write"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Yicoding/eco-image-picker.git"
  },
  "keywords": [
    "react",
    "react-components",
    "image-picker",
    "eco-image-picker"
  ],
  "files": [
    "dist"
  ],
  "dependencies": {
    "antd-mobile": "^2.3.4",
    "classnames": "^2.3.1",
    "compressorjs": "^1.0.7",
    "rc-field-form": "^1.21.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-file-viewer": "^1.2.1",
    "react-wx-images-viewer": "^1.0.6",
    "vconsole": "^3.9.1"
  },
  "devDependencies": {
    "@umijs/test": "^3.0.5",
    "babel-plugin-import": "^1.13.3",
    "babel-plugin-transform-remove-console": "^6.9.4",
    "compression-webpack-plugin": "^6.1.1",
    "dumi": "^1.0.16",
    "father-build": "^1.17.2",
    "gh-pages": "^3.0.0",
    "lint-staged": "^10.0.7",
    "prettier": "^2.2.1",
    "yorkie": "^2.0.0"
  }
}
